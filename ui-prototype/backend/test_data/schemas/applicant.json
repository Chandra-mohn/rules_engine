{
  "name": "applicant",
  "description": "Credit card applicant information",
  "is_active": true,
  "attributes": [
    {
      "name": "creditScore",
      "data_type": "number",
      "java_type": "int",
      "min_value": 300,
      "max_value": 850,
      "description": "Credit score (300-850)",
      "is_required": true
    },
    {
      "name": "age",
      "data_type": "number",
      "java_type": "int",
      "min_value": 18,
      "max_value": 120,
      "description": "Age in years",
      "is_required": true
    },
    {
      "name": "annualIncome",
      "data_type": "number",
      "java_type": "int",
      "min_value": 0,
      "description": "Annual income in dollars",
      "is_required": true
    },
    {
      "name": "monthlyIncome",
      "data_type": "number",
      "java_type": "int",
      "min_value": 0,
      "description": "Monthly income in dollars",
      "is_required": false
    },
    {
      "name": "employmentStatus",
      "data_type": "string",
      "java_type": "String",
      "allowed_values": ["employed", "unemployed", "self-employed", "retired", "student"],
      "description": "Employment status",
      "is_required": true
    },
    {
      "name": "employmentYears",
      "data_type": "number",
      "java_type": "int",
      "min_value": 0,
      "max_value": 50,
      "description": "Years of employment",
      "is_required": false
    },
    {
      "name": "applicationDate",
      "data_type": "date",
      "java_type": "LocalDate",
      "description": "Application submission date",
      "is_required": true
    }
  ]
}
