{
  "schemas": [
    {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "CUSTOM-FIELDS",
      "type": "object",
      "properties": {
        "RETAIL-CUSTOM": {
          "type": "object",
          "properties": {
            "HOBBY-INTEREST": {
              "type": "string",
              "maxLength": 30
            },
            "FAMILY-SIZE": {
              "type": "integer",
              "minimum": 0,
              "maximum": 99
            },
            "EDUCATION-LEVEL": {
              "type": "string",
              "maxLength": 2
            },
            "HOME-OWNERSHIP": {
              "type": "string",
              "maxLength": 1
            },
            "VEHICLE-COUNT": {
              "type": "integer",
              "minimum": 0,
              "maximum": 99
            },
            "PET-OWNERSHIP": {
              "type": "string",
              "maxLength": 1
            },
            "TRAVEL-FREQUENCY": {
              "type": "string",
              "maxLength": 2
            },
            "SOCIAL-MEDIA-ACTIVE": {
              "type": "string",
              "maxLength": 1
            }
          },
          "required": [
            "HOBBY-INTEREST",
            "FAMILY-SIZE",
            "EDUCATION-LEVEL",
            "HOME-OWNERSHIP",
            "VEHICLE-COUNT",
            "PET-OWNERSHIP",
            "TRAVEL-FREQUENCY",
            "SOCIAL-MEDIA-ACTIVE"
          ]
        },
        "BUSINESS-CUSTOM": {
          "type": "object",
          "properties": {
            "BUSINESS-TYPE": {
              "type": "string",
              "maxLength": 3
            },
            "EMPLOYEE-COUNT": {
              "type": "integer",
              "minimum": 0,
              "maximum": 999999
            },
            "YEARS-IN-BUSINESS": {
              "type": "integer",
              "minimum": 0,
              "maximum": 999
            },
            "ANNUAL-REVENUE-BAND": {
              "type": "string",
              "maxLength": 2
            },
            "INDUSTRY-SEGMENT": {
              "type": "string",
              "maxLength": 5
            },
            "INCORPORATION-STATE": {
              "type": "string",
              "maxLength": 2
            },
            "FISCAL-YEAR-END": {
              "type": "string",
              "maxLength": 4
            },
            "PUBLIC-COMPANY-FLAG": {
              "type": "string",
              "maxLength": 1
            }
          },
          "required": [
            "BUSINESS-TYPE",
            "EMPLOYEE-COUNT",
            "YEARS-IN-BUSINESS",
            "ANNUAL-REVENUE-BAND",
            "INDUSTRY-SEGMENT",
            "INCORPORATION-STATE",
            "FISCAL-YEAR-END",
            "PUBLIC-COMPANY-FLAG"
          ]
        }
      },
      "required": [
        "RETAIL-CUSTOM",
        "BUSINESS-CUSTOM"
      ],
      "x-variant-name": "BUSINESS-CUSTOM"
    }
  ],
  "copybook": "CUSTOMER-MASTER-RECORD",
  "group": "BUSINESS-CUSTOM",
  "variant_count": 1
}